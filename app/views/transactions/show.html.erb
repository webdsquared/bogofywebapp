<h2>Transaction</h2>

<p>
  <strong>Date:</strong>
  <%= @transaction.date %>
</p>
<p>
  <strong>Amount Spent:</strong>
  <%= @transaction.amount_spent %>
</p>
<p>
  <strong>Amount Saved:</strong>
  <%= @transaction.amount_saved %>
</p>

<p>
  <%= link_to "Edit", edit_transaction_path(@transaction) %> |
  <%= link_to "Destroy", @transaction, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", transactions_path %>
</p>
